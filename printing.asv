%d
n = -50:50;
xn1 =  sinc(n./3);
fft_xn1= fftshift(fft(xn1));
stem(n,abs(fft_xn1));
title('fft xn1 - Part c')
ylabel('X(e.^j\Omega)');
xlabel('n [sec]');

xn2 = cos(pi.*n./6)+sin(pi.*n./3);
fft_xn2= fftshift(fft(xn2));
stem(n,abs(fft_xn2));
title('fft xn2 ')
ylabel('X(e.^j\Omega)');
xlabel('n [sec]');